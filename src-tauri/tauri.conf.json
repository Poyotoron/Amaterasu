{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "amaterasu",
  "mainBinaryName": "amaterasu",
  "version": "0.4.1",
  "identifier": "com.poyotoron.amaterasu",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "amaterasu",
        "width": 1400,
        "height": 400,
        "useHttpsScheme": true
      }
    ]
  }
}